{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "edge": "10",
          "chrome": "64",
          "firefox": "60",
          "safari": "11.1"
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    ["@babel/preset-react"]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "helpers": true,
        "regenerator": true,
        "useEsModules": false
      }
    ]
  ]
}